- name: Initialize empty list for accumulation
  set_fact:
    sync_entries: []

- name: Append each item to the sync_entries list
  set_fact:
    sync_entries: "{{ sync_entries + [ { (item.split(',')[0].strip(\"(' \")): item.split(',')[1].strip(\") '\") } ] }}"
  loop: "{{ script_output.stdout_lines }}"

- name: Convert list of dictionaries to a single dictionary
  set_fact:
    last_sync_responses: "{{ last_sync_responses | default({}) | combine(item) }}"
  loop: "{{ sync_entries }}"
  
- name: Print the final dictionary output
  debug:
    var: last_sync_responses
