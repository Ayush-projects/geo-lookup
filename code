xcopy "\\NetworkShare\Scripts\CheckEthernet.ps1" "C:\Scripts\" /Y


powershell -Command "Set-ExecutionPolicy RemoteSigned -Scope LocalMachine -Force"

schtasks /Create /TN "CheckEthernetConnection" ^
/TR "powershell.exe -ExecutionPolicy Bypass -File \"C:\Scripts\CheckEthernet.ps1\"" ^
/SC ONSTART /RL HIGHEST /RU "SYSTEM"

